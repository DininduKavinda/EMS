@{
    ViewData["Title"] = "Index";
}
@model List<Attendance>
<div class="container">
    <div class="row pt-4 pb3">
        <div class="col-6">
            <h1 class="text-primary">All Attendance</h1>
        </div>
        <div class="col-6 text-end">
            <a asp-controller="Attendance" asp-action="Create" class="btn btn-primary">
                <i class="bi bi-plus-circle"></i> Add New
            </a>
        </div>
    </div>
    <table id="tblData" class="table table-striped" style="width:100%">
        <thead>

            <tr>
                <th>Name</th>
                <th>In Time</th>
                <th>In Image</th>
                <th>Out Time</th>
                <th>Out Image</th>
            </tr>
        </thead>

    </table>
    @section Scripts{
        @{
            <script src="~/js/TodayAttendance.js"></script>
        }
    }
</div>

