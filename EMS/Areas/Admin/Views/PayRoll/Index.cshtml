@{
    ViewData["Title"] = "Index";
}
@model List<PayRoll>
<div class="container">
    <div class="row pt-4 pb3">
        <div class="col-6">
            <h1 class="text-primary">Payed Salary Information</h1>
        </div>
    </div>
    <table id="tblData" class="table table-striped" style="width:100%">
        <thead>

            <tr>
                <th>C-ID</th>
                <th>Department</th>
                <th>Name</th>
                <th>Payed Date</th>
                <th>Position</th>
                <th>Basic Salary</th>
                <th>Deductions EPF 8%</th>
                <th>Worked Days</th>
                <th>Monthly Basic Salary</th>
                <th>Advanced</th>
                <th>Allowances</th>
                <th>Net Salary</th>
                <th>Action</th>
            </tr>
        </thead>
    </table>
    @section Scripts{
        <script src="~/js/PayRoll.js"></script>
    }
</div>

