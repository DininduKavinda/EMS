@{
    ViewData["Title"] = "Index";
}
@model Vehicle
<div class="container">
    <div class="row pt-4 pb3">
        <div class="col-6">
            <h1 class="text-primary">Vehicle Maintains</h1>
        </div>

        <!-- Modal -->
    </div>
    <table id="tblData" class="table table-striped" style="width:100%">
        <thead>

            <tr>
                <th>VehicleName</th>
                <th>VehicleNumber</th>
                <th>VehicleType</th>
                <th>Lisence</th>
                <th>Insurance</th>
                <th>Maintain</th>
                <th>Service</th>
                <th>Status</th>
            </tr>
        </thead>
    </table>
    @section Scripts {
        <script src="~/js/VehicleDetails.js"></script>
    }
</div>

